package Model;

import processing.core.PApplet;

public class EnemyModel {
	int posX;
	int posY;
	int tam;
	int vel;
	PApplet app;
	
	public EnemyModel(int posX, int posY, int tam, int vel, PApplet app) {
		this.posX=posX;
		this.posY=posY;
		this.tam=tam;
		
	}
	
	public void paintEne() {
		app.rect(posX, posY, tam + 5, tam);
		
	}
	
	

}
