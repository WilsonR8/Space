package Model;

import processing.core.PApplet;

public class Logic {
	
	EnemyModel Enemy;
	HeroModel Hero;
	public PApplet app;
	public Logic(PApplet app) {
		Enemy = new EnemyModel(100, 100, 10, 2, app);
		Hero = new HeroModel(250, 450, 10, 2, app);
		this.app=app;
	}
	
public void paint() {
	
	Enemy.paintEne();
	Hero.paintHero();
	
}



}
